{
  "name": "orient-client-ui",
  "version": "0.0.1",
  "description": "ORIENT: React-based Web Application for Clients",
  "author": "Andrew Nekrasov <bonerdelli@gmail.com>",
  "license": "UNLICENSED",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel serve",
    "clean": "rimraf ./dist/* ./.parcel-cache",
    "build": "run-s clean tsc parcel:build",
    "lint": "eslint ./src -f table --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src -f table --fix --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "generate": "generate-react component --",
    "generate:page": "generate-react component --type=page --",
    "parcel:build": "parcel build --no-cache --no-source-maps --log-level error",
    "pre-commit": "npm run lint:fix && npm run prettier:fix",
    "prepare": "husky install",
    "tsc": "tsc"
  },
  "alias": {
    "assets": "./assets",
    "config": "./config",
    "components": "./src/components",
    "library": "./src/library",
    "pages": "./src/pages"
  },
  "engines": {
    "node": "12.18.3 || >=14"
  },
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "targets": {
    "web": {
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "dependencies": {
    "antd": "4.19.5",
    "dayjs": "1.11.0",
    "easy-peasy": "5.0.4",
    "i18next": "21.6.14",
    "lodash": "4.17.21",
    "react": "16.14.0",
    "react-dom": "16.11.0",
    "react-i18next": "11.16.2",
    "react-router-dom": "5.3.0",
    "redux": "4.1.2",
    "redux-persist": "6.0.0",
    "superagent": "7.1.1"
  },
  "devDependencies": {
    "@parcel/config-default": "2.4.1",
    "@parcel/core": "2.4.1",
    "@parcel/packager-css": "2.4.1",
    "@parcel/transformer-less": "2.4.1",
    "@parcel/transformer-svg-react": "2.4.1",
    "@parcel/transformer-typescript-tsc": "2.4.1",
    "@parcel/transformer-yaml": "2.4.1",
    "@types/lodash": "4.14.181",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-router-dom": "5.3.3",
    "@types/superagent": "4.1.15",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "generate-react-cli": "7.1.0",
    "husky": "7.0.4",
    "js-yaml": "4.1.0",
    "less": "2.7.2",
    "lint-staged": "12.3.7",
    "npm-run-all": "4.1.5",
    "parcel": "2.4.1",
    "parcel-reporter-static-files-copy": "1.3.4",
    "prettier": "2.6.2",
    "process": "0.11.10",
    "rimraf": "3.0.2",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{js,ts,tsx,css,less,md,json}": "prettier --write"
  }
}

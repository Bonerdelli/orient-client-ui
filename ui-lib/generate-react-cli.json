{
  "usesTypeScript": true,
  "usesCssModule": true,
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "index": "templates/default/index.ts",
        "component": "templates/default/TemplateName.tsx",
        "style": "templates/default/TemplateName.style.less"
      },
      "path": "components",
      "withStyle": true,
      "withIndex": true,
